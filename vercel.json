{
  "version": 2,
  "routes": [
    { "src": "/socket.io/", "dest": ":3000/socket.io/" },
    { "src": "/(.*)", "dest": "http://localhost:3000/$1" }
  ]
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60,
        "memory": 128
      }
    }
  ],
  "routes": [{ "src": "/.*", "dest": "/index.js" }]
}
